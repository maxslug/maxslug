#!/bin/bash

G=$HOME/cad/maxslug
RC_FILES="vimrc tmux.conf bashrc bashrc.local bash_profile tclshrc dircolors"

for f in $RC_FILES; do
  if [[ ! -e ~/$f ]] ; then
    ln -sf $G/rc/$f ~/.$f
  fi
done

ln -sf $G/bin ~/

